{
    "root": true,
    "env": {
        "node": true
    },
    "extends": [
      "plugin:vue/vue3-recommended",
      "eslint:recommended",
      "@vue/typescript/recommended",
      "@vue/prettier"
    ],
    "parserOptions": {
        "ecmaVersion": 2021
    },
    "plugins": [
    ],
    "rules": {
      "quotes": ["error", "single"],
      "prettier/prettier": ["error", { "singleQuote": true, "semi": true }]
    }
  }
